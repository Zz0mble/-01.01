<Window x:Class="ESUN.Tool.AddJour"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ESUN.Tool"
        mc:Ignorable="d"
        Title="AddJour" Height="450" Width="800">
    <Grid>
        <StackPanel  HorizontalAlignment="Center" VerticalAlignment="Center">
            <ComboBox Name="Idd" DisplayMemberPath="Id" SelectedItem="{Binding Student}" Width="100" SelectionChanged="Idd_SelectionChanged"/>
            <Button Content="Back" Click="Button_Click"/>
        </StackPanel>
    </Grid>
</Window>
