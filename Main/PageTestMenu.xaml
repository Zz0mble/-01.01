<Page x:Class="ESUN.Main.PageTestMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ESUN.Main"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="PageTestMenu">

    <Grid Background="White">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Введение" Width="100" Height="30" Name="Test" Click="Test_Click" Margin="5"/>
            <Button Content="Молекулы" Width="100" Height="30" Name="Test1" Click="Test1_Click" Margin="5"/>
            <Button Content="Назад" Width="100" Height="30" Name="Back" Click="Back_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Page>
